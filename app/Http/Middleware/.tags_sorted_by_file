res	LoginMiddleWare.php	/^        \/\/ $res = $this->db->fetchRow("select time_out from userinfo where token='{$token}'");$/;"	v
rooms	LoginMiddleWare.php	/^        \/\/ $rooms = DB::table('food_area')->where("partner_id",$p_id)->get()->toArray();$/;"	v
