     /**
     * 对象转换数组
     *
     * @param $e StdClass对象实例
     * @return array|void
     */
     public function objectToArray($e)
     {
         $e = (array)$e;
         foreach ($e as $k => $v) {
             if (gettype($v) == 'resource') return;
             if (gettype($v) == 'object' || gettype($v) == 'array')
                 $e[$k] = (array)$this->objectToArray($v);
         }
         return $e;
     }

