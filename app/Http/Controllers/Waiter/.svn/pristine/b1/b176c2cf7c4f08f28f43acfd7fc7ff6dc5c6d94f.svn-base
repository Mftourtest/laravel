        $prr = [];
        foreach($cateinfo as $i=>$cate){
            $categorys[$i] =  DB::table('food')->where('cate_id',$cate->id)->orderBy('display_order','desc')->get()->toArray();
            $brr = $this->objectToArray($categorys[$i]);
                foreach ( $brr as $j => $food) {
